<html>
<head>
	<title itemprop="name">Rolling Sweets</title>
	<meta charset="UTF-8" />
	<meta id="viewport" name="viewport" content="width=device-width, initial-scale=1" />
	<style>{{= it.style }}</style>
</head>
<body>
	<div class="logo">
		<div class="logo__frame">
			<img alt="Rolling Sweets" src="/img/logo.png" />
		</div>
	</div>
	<div class="map" id="map"></div>
    <script>
      function initMap() {
        var uluru = { lat: 54.797439, lng: 18.40031 };
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 15,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAesqkbSXzjrgIEEPbooSOE75tlPPJTx1w&callback=initMap">
    </script>
	<div class="v"></div>
	<script type="text/javascript">{{= it.script }}</script>
	<script type="text/javascript">
